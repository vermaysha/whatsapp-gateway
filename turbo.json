{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test:ci": {
      "cache": false
    },
    "db:migrate:deploy": {},
    "db:push": {},
    "db:seed": {},
    "generate": {
      "dependsOn": [
        "^generate"
      ]
    }
  }
}
